<launch>
	<param name="/use_sim_time" value="true" />
    <arg name="start_spot" default="false" />
    <arg name="start_jackal" default="true" />

    <node pkg="asa_ros_commander" name="asa_ros_talker" type="talker.py" output="screen"/>
    <node pkg="asa_ros_commander" name="asa_ros_commander" type="commander.py" output="screen"/>
    
    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />
    <!--Spot-->
    <include file="$(find spot_driver)/launch/driver.launch" pass_all_args="true" if="$(arg start_spot)"/>

    
    <!--Jackal-->
	<include file="$(find jackal_navigation)/launch/odom_navigation_demo.launch" if="$(arg start_jackal)"/>

  	<node name="rviz" pkg="rviz" type="rviz" args="-d /home/user/custom.rviz" />
    <include file="$(find ms_jackal_demo)/launch/jackal_custom_empty_world.launch" pass_all_args="true"  if="$(arg start_jackal)">
		<arg name="x" value="0" />
		<arg name="y" value="0" />
		<arg name="z" value="1.0" />
		<arg name="yaw" value="0" />
		<arg name="config" value="base" />
		<arg name="joystick" value="false" />
		<arg name="gui" value="false" />

	</include>


</launch>